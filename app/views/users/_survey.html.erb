<%= label_tag(:Ambient_andDrone_andSynth) %>
<%= check_box_tag(:Metal_and_Hardcore) %>
<%= label_tag(:Metal_and_Hardcore) %>
<%= check_box_tag(:Pop_Country) %>
<%= label_tag(:Pop_Country) %>
<%= check_box_tag(:Disney_radio) %>
<%= label_tag(:Disney_radio) %>
<%= check_box_tag(:Gospel) %>
<%= label_tag(:Gospel) %>
<%= check_box_tag(:Classic_rock) %>
<%= label_tag(:Classic_rock) %>
<%= check_box_tag(:IndieRock) %>
<%= label_tag(:IndieRock) %>
